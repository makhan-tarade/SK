<div class="modal fade" id="exampleModalCenterContactInfo" tabindex="-1" role="dialog"
    aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header pop1-header">
                <h5 class="modal-title pop1-h5" id="exampleModalLongTitle">Contact Info </h5>
                <button type="button" class="close pop1-close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <% if (contact){%>

                    <!-- enctype="multipart/form-data" -->
                    <form action="/contact_info_edit/<%=contact._id%>" method="post">


                        <div class="form-group">
                            <p class="pop1-p">Address</p>
                            <div class="form-floating">
                                <textarea name="address" class="form-control" placeholder="" id="floatingTextarea2"
                                    style="height: 100px" required><%=contact.address%></textarea>
                            </div>

                        </div>

                        <div class="form-group" style="margin-top:4%">
                            <p class="pop1-p">Mobile Number</p>

                            <input type="text" name="mobile"
                                value="<%= contact.contact_mobile  ? contact.contact_mobile : contact.mobile %>"
                                class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder=""
                                required>

                        </div>

                        <div class="form-group" style="margin-top:4%">
                            <p class="pop1-p">Email</p>

                            <input type="email" style="text-transform:none" name="email"
                                value="<%= contact.contact_email  ? contact.contact_email : contact.email %>"
                                class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder=""
                                required>

                        </div>

                        <div class="form-group" style="margin-top:4%">
                            <p class="pop1-p">Birthday</p>

                            <input type="date" style="text-transform:none" name="birthday" value="<%=contact.birthday%>"
                                class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder=""
                                required>

                        </div>

                        <!-- <div class="form-group" style="margin-top:4%">
                            <p class="pop1-p">Gender</p>

                            <input type="text" name="gender" value="<%= contact.gender %>" class="form-control"
                                id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="" required>

                        </div>

                        <div class="form-group" style="margin-top:4%">
                            <p class="pop1-p">Blood Group</p>

                            <input type="text" name="blood_group" value="<%= contact.blood_group %>" class="form-control"
                                id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="" required>

                        </div> -->

                        <div class="form-group">
                            <p class="pop1-p">Gender</p>

                            <select name="gender" class="form-select form-select-lg mb-3"
                                aria-label=".form-select-lg example" id="ddlProducts">

                                <option selected class="please-select">Please select gender</option>
                                <option value="male">Male</option>
                                <option value="female">Female</option>
                                <option value="other">other</option>

                            </select>

                        </div>

                        <div class="form-group">
                            <p class="pop1-p">Blood Group</p>

                            <select name="blood_group" class="form-select form-select-lg mb-3"
                                aria-label=".form-select-lg example" >

                                <option selected class="please-select">Please select your blood group</option>
                                <option value="A+">A+</option>
                                <option value="A-">A-</option>

                                <option value="B+">B+</option>
                                <option value="B-">B-</option>
                              
                                <option value="AB+">AB+</option>
                                <option value="AB-">AB-</option>
                             
                                <option value="O+">O+</option>
                                <option value="O-">O-</option>
                              
                            </select>

                        </div>


                        <div class="modal-footer pop1-foot">

                            <button type="button" class="btn btn-secondary pop1-btn2"
                                data-dismiss="modal">Cancel</button>
                            <button type="submit" onclick="window.location.reload()"
                                class="btn btn-primary pop1-btn1 new-btn1">Save</button>

                        </div>

                    </form>

                    <%}%>




            </div>
        </div>

    </div>
</div>
<!-- create contact info popup end -->