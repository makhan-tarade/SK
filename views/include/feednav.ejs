            <p><a href="/logout"><i class="bi bi-box-arrow-right"></i> Logout</a></p>
        </div>
    </div>

</div>
<!-- profile btn popup end-->

<!-- notification btn popup start-->
<div id="notification_opn" style="display: none">
    <div class="profile-btn-menu  profile-btn-menu1 profile-btn-menu2">
        <div class="profile-btn-menu-r1">
            <button type="button" class="btn-close btn-close2" onclick="notification_close()"></button>
        </div>
        <span class="notification-h">Notification</span>

        <hr style="height: 2px">

        <div class="row" id="notification-bar">
            
            <% if(notifications){ notifications.forEach(function(n,ni){%>
                <% if(n.action === 'pending') {%>
            <div class="col-3">

                <div class="notification-img">
                    <img src="<%= n.sender.profile_pic  %>" alt="">
                </div>
            </div>
            <div class="col-9">
                <div class="request">
                    <p> You are invited friend request by <span> <%= n.sender.fname + ' ' + n.sender.lname %> </span> </p>
                        <a href="/friend-response/?id=<%= n._id %>&action=accept">Accept</a>
                        <a href="/friend-response/?id=<%= n._id %>&action=reject">Decline</a>
                        <!-- <button class="btn">Accept</button>
                         <span> <button>Decline</button> </span> -->
           

                </div>
            </div>
            <%} else if(n.action === 'accept') {%>
                
                <%} %>
            <%})} %>

        </div>

    </div>


</div>
<!-- notification btn popup end-->

<script>
    // profile image - profile menu
    function profile_opn() {
        document.getElementById("profile_opn").style.display = "block";
    }

    function profile_close() {
        document.getElementById("profile_opn").style.display = "none";
    }


    function notification_opn() {
        document.getElementById("notification_opn").style.display = "block";
    }


    function notification_close() {
        document.getElementById("notification_opn").style.display = "none";
    }
</script>