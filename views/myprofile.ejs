                    <!-- Modal -->
                    <div class="modal fade" id="add-volunteer" tabindex="-1" role="dialog"
                        aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content">
                                <div class="modal-header pop1-header">
                                    <h5 class="modal-title pop1-h5" id="exampleModalLongTitle">Add Volunteers Experience
                                    </h5>
                                    <button type="button" class="close pop1-close" data-dismiss="modal"
                                        aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">


                                    <form action="/add-volunteer" method="post">
                                        <div class="form-group">
                                            <p class="pop1-p">Organization *</p>

                                            <input type="text" name="organization" class="form-control"
                                                id="exampleInputEmail1" aria-describedby="emailHelp"
                                                placeholder="Enter organization ">

                                        </div>

                                        <div class="form-group">
                                            <p class="pop1-p">Role *</p>

                                            <input type="text" name="role" class="form-control" id="exampleInputEmail1"
                                                aria-describedby="emailHelp" placeholder="Enter Role ">

                                        </div>

                                        <div class="form-group">
                                            <p class="pop1-p">Cause</p>

                                            <select name="cause" class="form-select form-select-lg mb-3"
                                                aria-label=".form-select-lg example" id="ddlProducts">

                                                <option selected class="please-select">Please select</option>
                                                <option value="Animal Welfare">Animal Welfare</option>
                                                <option value="Art And Culture">Art And Culture</option>
                                                <option value="Children">Children</option>
                                                <option value="Civil Rights and Social Actions">Civil Rights and Social
                                                    Actions</option>
                                                <option value="Economic Empowerment">Economic Empowerment</option>
                                                <option value="Education">Education</option>
                                                <option value="Environment">Environment</option>
                                                <option value="Health">Health</option>
                                                <option value="Human Rights">Human Rights</option>
                                                <option value="Disaster and Humanitarian Relieg">Disaster and
                                                    Humanitarian Relieg</option>
                                                <option value="Politics">Politics</option>
                                                <option value="Poverty Alleviation">Poverty Alleviation</option>
                                                <option value="Science and Technology">Science and Technology</option>
                                                <option value="Veteran Support">Veteran Support</option>
                                            </select>

                                        </div>

                                        <div class="form-check">
                                            <input name="check" class="form-check-input" type="checkbox"
                                                value="I am currently volunteering in this role" id="flexCheckChecked"
                                                checked>
                                            <label class="form-check-label new-p" for="flexCheckChecked">
                                                I am currently volunteering in this role
                                            </label>
                                        </div>



                                        <div class="row">
                                            <div class="form-group form-group5 col-6">
                                                <div class="form-group">
                                                    <p class="pop1-p">Start Date</p>

                                                    <input type="date" name="start_date" class="form-control"
                                                        aria-describedby="emailHelp" placeholder="">

                                                </div>
                                            </div>

                                            <div class="form-group col-6">
                                                <div class="form-group">
                                                    <p class="pop1-p">Start time</p>

                                                    <input type="time" name="start_time" class="form-control"
                                                        aria-describedby="emailHelp" placeholder="">

                                                </div>

                                            </div>


                                        </div>

                                        <div class="row">
                                            <div class="form-group form-group5 col-6">
                                                <div class="form-group">
                                                    <p class="pop1-p">End Date (or expected)</p>

                                                    <input type="date" name="end_date" class="form-control"
                                                        aria-describedby="emailHelp" placeholder="">

                                                </div>
                                            </div>

                                            <div class="form-group col-6">
                                                <div class="form-group">
                                                    <p class="pop1-p">End time</p>

                                                    <input type="time" name="end_time" class="form-control"
                                                        aria-describedby="emailHelp" placeholder="">

                                                </div>

                                            </div>
                                        </div>

                                        <div class="form-group">
