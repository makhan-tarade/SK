                                            <div class="col-lg-2 col-md-2 col-sm-6">
                                                <div class="pdf">
                                                    <div class="row">
                                                        <div class="col-7">
                                                            <div class="pdf-img">
                                                                <img src="assets/img/pdf.png" alt="">
                                                            </div>


                                                        </div>
                                                        <div class="col-5">
                                                            <div class="pdf-download">
                                                                <a href="media/health/<%= health.health[i].attachment %>"
                                                                    download>
                                                                    <p><i class="bi bi-download"></i></p>
                                                                </a>
                                                                <p><i class="bi bi-share-fill"></i></p>
                                                                <!-- <p><i class="bi bi-trash3-fill"></i></p> -->
                                                                <a href="/delete-health/<%= i%>"
                                                                    onclick="return confirm('Are you sure you want to delete this attachment?');">
                                                                    <p><i class="bi bi-trash3-fill"></i></p>
                                                                </a>
                                                            </div>
                                                        </div>

                                                    </div>

                                                </div>
                                            </div>
                                            <%}}%>


                                                <div class="col-lg-2 col-md-2 col-sm-6" data-toggle="modal"
                                                    data-target="#reportModal">
                                                    <div class="pdf">
                                                        <div class="row">
                                                            <p class="pdf-plus"><i class="bi bi-plus-lg"></i></p>

                                                        </div>

                                                    </div>
                                                </div>

                                    </div>


                                    <div class="row">
                                        <p class="health-p3">Prescription</p>

                                        <% if (health.prescription){ for(var i=0; i < health.prescription.length; i++) {
                                            %>
                                            <div class="col-lg-2 col-md-2 col-sm-6">
                                                <div class="pdf">
                                                    <div class="row">
                                                        <div class="col-7">
                                                            <div class="pdf-img">
                                                                <img src="assets/img/pdf.png" alt="">
                                                            </div>

                                                        </div>
                                                        <div class="col-5">
                                                            <div class="pdf-download">
                                                                <a href="media/health/<%= health.prescription[i].attachment %>"
                                                                    download>
                                                                    <p><i class="bi bi-download"></i></p>
                                                                </a>
                                                                <p><i class="bi bi-share-fill"></i></p>
                                                                <a href="/delete-prescription/<%= i%>"
                                                                    onclick="return confirm('Are you sure you want to delete this attachment?');">
                                                                    <p><i class="bi bi-trash3-fill"></i></p>
                                                                </a>
                                                            </div>
                                                        </div>

                                                    </div>

                                                </div>
                                            </div>
                                            <%}}%>

                                                <div class="col-lg-2 col-md-2 col-sm-6" data-toggle="modal"
                                                    data-target="#exampleModalCenter666">
                                                    <div class="pdf">
                                                        <div class="row">
                                                            <p class="pdf-plus"><i class="bi bi-plus-lg"></i></p>

                                                        </div>

                                                    </div>
                                                </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-11">
                                            <p class="health-p3">Brief About Doctor</p>
                                        </div>
                                        <div class="col-1  mt-4" data-toggle="modal" data-target="#addDoctorModal">
                                            <span class="plus"><i class="bi bi-plus-lg"></i></span>
                                        </div>

                                        <% if(doctor) { doctor.forEach(function(d,i){ %> 
                                            <div class="col-lg-6 col-md-6 col-sm-12">
                                                <div class="pdf1">
                                                    <div class="row">
                                                        <div class="col-3">
                                                            <div class="pdf1-img">
                                                                <img src="<%= d.profile_pic %>"
                                                                    alt="">
                                                            </div>

                                                        </div>
                                                        <div class="col-7">
                                                            <div class="pdf1-download">
                                                                <p class="pdf-text1">Dr. <%= d.name %>
                                                                </p>
                                                                <p class="pdf-text2"><%= d.degination %></p>

                                                            </div>
                                                        </div>
                                                        <div class="col-1 pencil">
                                                            <p class="pdf-i" data-toggle="modal" data-target="#edit-DoctorModal-<%= d._id %>"> <img src="assets/img/edit.svg" alt=""></p>
                                                        </div>
                                                        <div class="col-1">
                                                            <p class="pdf-i1"> <a href="/doctor-delete/<%= d._id %>"><i class="bi bi-trash"></i></a>
                                                            </p>
                                                        </div>

                                                        <p class="pdf-text3">
                                                            <%= d.description %>
                                                        </p>
                                                        <div class="row"></div>
                                                        <div class="col-2 pdf-icon">
                                                            <p><i class="bi bi-chat-dots"></i></p>
                                                        </div>
                                                        <div class="col-2 pdf-icon">
                                                            <p><i class="bi bi-telephone"></i></p>
                                                        </div>
                                                        <div class="col-2 pdf-icon">
                                                            <p><i class="bi bi-camera-video"></i></p>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                           
                                            <%})} %>
                                               
                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>
                    <!-- Health end -->

                    <!-- Community pillar start -->
                    <div id="community-pillar">
                        <!-- <h2 class="sec-heading">My Hub</h2> -->
                        <div class="myhub-sec">
                            <div class="my-hub-menu">
                                <ul>
                                    <li class="li3" onclick="family()">Family</li>
                                    <li class="li1" onclick="health()">Health</li>
                                    <li class="li4" onclick="doctor()">Doctor</li>
                                    <li class="li2" onclick="community_pillar()">Community Pillar</li>
                                    <li class="li5" onclick="public_service()">Public Service</li>
                                </ul>

                            </div>


                            <div class="community-pillar-tab">

                                <div class="action-sec">
                                    <div class="search-com-pil">
                                        <i class="bi bi-search"></i> <input type="text" placeholder="Search..." id="searchinputpillar" onkeyup="searchpillarFunction()" > <i
                                            class="bi bi-mic" style="color:  #225C7B "></i>
                                    </div>
                                    <button class="btn btn-primary" data-toggle="modal"
                                        data-target="#exampleModalCenter2">Invite</button>
                                    <!-- <button class="btn btn-primary"><i class="bi bi-camera-video"></i> Group Call</button> -->

                                    <label for="">Sort by: &nbsp; &nbsp;</label>
                                    <select class="form-select" aria-label="Default select example">
                                        <option selected>Recently added</option>
                                        <option value="1">One</option>
                                        <option value="2">Two</option>
                                        <option value="3">Three</option>
                                    </select>
                                </div>
                                <br>

                                <div class="row" style="padding: 0 20px;">
                                   
                                    <% users.forEach(function(c, i){%>                                       
                                        <% if(c.degination && c.degination !='' && c.degination != 'MBBS' && c.degination != 'MD' && c.degination != 'BDS'){%> 
                                        <div class="col-lg-4">
                                            <div class="c-p-card">
                                                <center>
                                                    <div class="img-center">
                                                        <img src="<%= c.profile_pic  ? c.profile_pic : 'assets/img/avtar.png' %>" alt="">
                                                    </div>
                                                </center>
                                                <div class="contentpillar"></div>
                                                <h2><%= c.fname+' '+c.lname%></h2>
                                                <h6><%= c.mobile%></h6>
                                                <p><%= c.degination%></p>
                                               
                                               </div>
                                                    <div class="btn-sec1">
                                                        <button class="btn btn-primary"><i class="bi bi-chat-dots"></i></button>
                                                        <button class="btn btn-primary"><i class="bi bi-telephone"></i></button>
                                                        <button class="btn btn-primary"><i class="bi bi-camera-video"></i></button>
                                                        <!-- <form action="?remove-friends/<%= myid%>" method="post" style="display: inline-block;">
                                                            <button type="submit" class="btn btn-primary new-dlt"
                                                                onclick="return confirm('Are you sure you want to delete this konnect?');"><i
                                                                    class="bi bi-trash3"></i></button>
                                                        </form> -->
                                                        <button type="submit" class="btn btn-primary new-dlt"><i class="bi bi-trash3"></i></button>
                                                    </div>                                                                                          
                                              
                            
                                            </div>
                                            
                                        </div>
                                        <%} %>
                                        <%}) %>
                                   
                                </div>
                                <br>

                            </div>
                        </div>
                    </div>
                    <!-- Community pillar end -->


                    <!-- Family tab start -->
                    <div id="family">
                        <!-- <h2 class="sec-heading">My Hub</h2> -->
                        <div class="myhub-sec">
                            <div class="my-hub-menu">
                                <ul>
                                    <li class="li3" onclick="family()">Family</li>
                                    <li class="li1" onclick="health()">Health</li>
                                    <li class="li4" onclick="doctor()">Doctor</li>
                                    <li class="li2" onclick="community_pillar()">Community Pillar</li>
                                    <li class="li5" onclick="public_service()">Public Service</li>
                                </ul>

                            </div>


                            <div class="community-pillar-tab">

                                <div class="action-sec">
                                    <div class="search-com-pil">
                                        <i class="bi bi-search"></i> <input type="text" placeholder="Search..." id="searchinput" onkeyup="searchFunction()" > <i
                                            class="bi bi-mic" style="color:  #225C7B "></i>
                                    </div>
                                    <button class="btn btn-primary" data-toggle="modal"
                                        data-target="#exampleModalCenter2">Invite</button>
                                    <!-- <button class="btn btn-primary"><i class="bi bi-camera-video"></i> Group Call</button> -->

                                    <label for="">Sort by: &nbsp; &nbsp;</label>
                                    <select class="form-select" aria-label="Default select example">
                                        <option selected>Recently added</option>
                                        <option value="1">One</option>
                                        <option value="2">Two</option>
                                        <option value="3">Three</option>
                                    </select>
                                </div>
                                <br>

                                <div class="row" style="padding: 0 20px;">
                                        <% if(my_kon){ my_kon.konnects.forEach(function(k,i){%>
                                            
                                            <% if(k.relation === 'friend'){ %>                                         
                                                <div class="col-lg-4">
                                                    <div class="c-p-card">
                                                        <center>
                                                            <div class="img-center">
                                                                <img src="<%= k.user.profile_pic  ? k.user.profile_pic : 'assets/img/avtar.png' %>"
                                                                    alt="">
                                                            </div>
                                                        </center>
                                    
                                                        <div class="content">
                                                        <h2>
                                                            <%= k.user.fname +' '+ k.user.lname %></h2>
                                                <h6><%= k.user.mobile %></h6>
                                                <p><%= k.user.address %></p> </div>
                                                <button class="btn btn-primary family-btn family-btn-family"
                                                    data-toggle="modal" data-target="#<%= k.user._id %>">Add
                                                    Family</button>
                                    
                                            </div>
                                        </div>
                                        <%} else if (k.relation !== 'friend'){%>
                                            <div class="col-lg-4">
                                                <div class="c-p-card">
                                                    <center>
                                                        <div class="img-center">
                                                            <img src="<%= k.user.profile_pic  ? k.user.profile_pic : 'assets/img/avtar.png' %>"
                                                                alt="">
                                                        </div>
                                                    </center>
                                
                                                    <div class="content">
                                                    <h2>
                                                        <%= k.user.fname +' '+ k.user.lname %></h2>
                                            <h6><%= k.user.mobile %></h6>
                                            <p><%= k.user.address %></p> </div>
                                            <div class="btn-sec1">
                                                <button class="btn btn-primary"><i class="bi bi-chat-dots"></i></button>
                                                <button class="btn btn-primary"><i class="bi bi-telephone"></i></button>
                                                <button class="btn btn-primary"><i
                                                        class="bi bi-camera-video"></i></button>
                                                <button class="btn btn-primary"><i class="bi bi-trash3"></i></button>
                                            </div>
                                
                                        </div>
                                    </div>
                                            <%}%>
                                        <%})}%>
                                    
                                        
                                    

                                    

                                </div>
                                <br>

                            </div>
                        </div>
                    </div>
                    <!-- Family tab end -->

                    <!-- Doctor tab start -->
                    <div id="doctor">
                        <!-- <h2 class="sec-heading">My Hub</h2> -->
                        <div class="myhub-sec">
                            <div class="my-hub-menu">
                                <ul>
                                    <li class="li3" onclick="family()">Family</li>
                                    <li class="li1" onclick="health()">Health</li>
                                    <li class="li4" onclick="doctor()">Doctor</li>
                                    <li class="li2" onclick="community_pillar()">Community Pillar</li>
                                    <li class="li5" onclick="public_service()">Public Service</li>
                                </ul>

                            </div>


                            <div class="community-pillar-tab">

                                <div class="action-sec">
                                    <div class="search-com-pil">
                                        <i class="bi bi-search"></i> <input type="text" placeholder="Search..." id="searchinputdoctor" onkeyup="searchdoctorFunction()" ><i
                                            class="bi bi-mic" style="color:  #225C7B "></i>
                                    </div>
                                    <button class="btn btn-primary" data-toggle="modal"
                                        data-target="#exampleModalCenter2">Invite</button>
                                    <!-- <button class="btn btn-primary"><i class="bi bi-camera-video"></i> Group Call</button> -->

                                    <label for="">Sort by: &nbsp; &nbsp;</label>
                                    <select class="form-select" aria-label="Default select example">
                                        <option selected>Recently added</option>
                                        <option value="1">One</option>
                                        <option value="2">Two</option>
                                        <option value="3">Three</option>
                                    </select>
                                </div>
                                <br>

                                <div class="row" style="padding: 0 20px;">

                                    <% if(users.length>0){ users.forEach(function(u,i){  %>
                                        <% if (u.degination){%>
                                        <% if(u.degination === 'doctor' || u.degination === 'MBBS' || u.degination === 'MD' || u.degination === 'BDS'){ %>
                                    <div class="col-lg-4">
                                        <div class="c-p-card">
                                            <center>
                                                <div class="img-center">
                                                    <img src="assets/img/shivaji2.jpg" alt="">
                                                </div>
                                            </center>
                                            <div class="contentdoctor"></div>
                                            <h2><%= u.fname +''+ u.lname %></h2>
                                            <h6><%= u.mobile %></h6>
                                            <p><%= u.degination %></p> </div>
                                            <!-- <button class="btn btn-primary family-btn family-btn-family" data-toggle="modal"
                                            data-target="#exampleModalCenter3">Add Family</button> -->
                                            <div class="btn-sec1">
                                                <button class="btn btn-primary"><i class="bi bi-chat-dots"></i></button>
                                                <button class="btn btn-primary"><i class="bi bi-telephone"></i></button>
                                                <button class="btn btn-primary"><i
                                                        class="bi bi-camera-video"></i></button>
                                                <button class="btn btn-primary"><i class="bi bi-trash3"></i></button>
                                            </div>

                                        </div>
                                    </div>
                                    <% }} %>
                                   
                                    <%})}%>

                                </div>
                                <br>

                            </div>
                        </div>
                    </div>
                    <!-- Doctor tab end -->

                    <!-- Public service tab start -->
                    <div id="public-service">
                        <!-- <h2 class="sec-heading">My Hub</h2> -->
                        <div class="myhub-sec">
                            <div class="my-hub-menu">
                                <ul>
                                    <li class="li3" onclick="family()">Family</li>
                                    <li class="li1" onclick="health()">Health</li>
                                    <li class="li4" onclick="doctor()">Doctor</li>
                                    <li class="li2" onclick="community_pillar()">Community Pillar</li>
                                    <li class="li5" onclick="public_service()">Public Service</li>
                                </ul>

                            </div>


                            <div class="community-pillar-tab">

                                <div class="action-sec">
                                    <div class="search-pub-service" style="width: 28%;">
                                        <i class="bi bi-search"></i> <input type="text" placeholder="Search by Zipcode">
                                    </div>

                                    <div class="search-pub-service search-pub-service2"
                                    style="margin-left: 0; width:28%">
                                    <i class="bi bi-search"></i> <input type="text"
                                        placeholder="Search/Add by name"id="searchinputpublic" onkeyup="searchpublicFunction()" >
                                </div>

                                    <label class="label2" for="">Sort by:</label>
                                    <select class="form-select select2" aria-label="Default select example">
                                        <option selected>Recently added</option>
                                        <option value="1">One</option>
                                        <option value="2">Two</option>
                                        <option value="3">Three</option>
                                    </select>
                                </div>
                                <br>

                                <div class="row" style="padding: 0 20px;">
                                    <% if(pub){ %>
                                        <% pub.forEach(function(c,i){%>
                                    <div class="col-md-6 col-sm-12">

                                        <div class="service-sec">
                                            <div class="contentpublic">
                                            <div class="row">
                                                <div class="col-4">
                                                    <div class="img-service">
                                                        <img src="/<%= c.service_pic %>" alt="public service pic">
                                                    </div>
                                                </div>
                                                <div class="col-8">
                                                    <div class="service-main">
                                                        <h1 class="service1">   <%= c.service_no%></h1>
                                                        <p class="service2">  <%= c.service_name%></p>
                                                        <p class="service3"> <%= c.service_address%></p>
                                                    </div>
                                                </div>
                                            </div>
                                            </div>
                                        </div>

                                    </div>

                                    <%}) %>
                                    <% } %>
                                </div>
                                <br>

                            </div>
                        </div>
                    </div>
