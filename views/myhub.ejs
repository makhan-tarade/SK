        <section class="container">
            <div class="row">
                <!-- left section start -->
                <div class="col-lg-8 space-top1">

                    <!-- Health start -->
                    <div id="health">
                        <!-- <h2 class="sec-heading">My hub</h2> -->
                        <div class="myhub-sec">
                            <div class="my-hub-menu">
                                <ul>
                                    <li class="li3" onclick="family()">Family</li>
                                    <li class="li1" onclick="health()">Health</li>
                                    <li class="li4" onclick="doctor()">Doctor</li>
                                    <li class="li2" onclick="community_pillar()">Community Pillar</li>
                                    <li class="li5" onclick="public_service()">Public Service</li>
                                    <!-- <li class="plusleft"><span
                                            class="plus"><i class="bi bi-plus-lg"></i></span></li> -->
                                </ul>

                            </div>


                            <div class="community-pillar-tab" style="padding-bottom: 20px;">
                                <div class="health-section">
                                    <div class="row">
                                        <div class="col-md-3 ">
                                            <div>
                                                <p class="health-p">Name</p>
                                            </div>
                                            <div class="health-p2">
                                                <p><% if(username){%><%= username %><%}%></p>
                                            </div>
                                        </div>
                                        <div class="col-md-4 ">
                                            <div>
                                                <p class="health-p">Birthdate:</p>
                                            </div>
                                            <div class="health-p2">
                                                
                                                <p><% if(dob) {%><%= dob %><%}%></p>
                                            </div>
                                        </div>
                                        <div class="col-md-2 ">
                                            <div>
                                                <p class="health-p">Gender:</p>
                                            </div>
                                            <div class="health-p2">
                                                <p><% if(contact) {%> <%= contact.gender %> <%}%></p>
                                            </div>
                                        </div>
                                        <div class="col-md-3 ">
                                            <div>
                                                <p class="health-p">Blood Group:</p>
                                            </div>
                                            <div class="health-p2">
                                                <p><% if(contact) {%> <%= contact.blood_group %> <%}%></p>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <p class="health-p3">Report</p>



