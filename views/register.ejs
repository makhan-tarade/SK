    <style>
        body {
            background-color: #F1EFF0;
            margin: 0;
            padding: 0;
        }

        .register_as {
            font-family: 'Inter';
            font-style: normal;
            font-weight: 700;
            font-size: 30px;
            line-height: 36px;

            color: #225C7B;
            text-align: center;
            margin-top: 3%;
        }

        .card {
            border: transparent;
        }

        .card10 {
            margin-left: 20%;
        }

        .card20 {
            margin-right: 20%;
        }



        .register-img {
            display: inline-block;
            width: 200px;
            height: 200px;
            /* margin: auto; */
            margin-left: 28%;

        }

        .register-heading {
            font-family: 'Inter';
            font-style: normal;
            font-weight: 600;
            font-size: 30px;
            line-height: 36px;
            text-align: center;

            color: #225C7B;
            margin-bottom: -2%;
        }

        .register-heading a {
            text-decoration: none;
            color: #225C7B;
        }

        .register-pera {
            font-family: 'Inter';
            font-style: normal;
            font-weight: 500;
            font-size: 18px;
            line-height: 22px;
            text-align: center;

            color: #444444;
        }

        .register-img img {
            width: 100%;
            height: 100%;
        }

        .login-now {
            font-family: 'Inter';
            font-style: normal;
            font-weight: 400;
            font-size: 16px;
            line-height: 19px;
            text-align: center;

            color: #444444;
            margin-top: 30px;
        }

        .login-now span {
            color: #225C7B;
            font-weight: 600;
        }

        .login-now span a {
            text-decoration: none;
            color: #225C7B;
        }



        #footer {
            background: #FFFFFF;
            font-family: 'Inter';
            font-style: normal;
            font-weight: 500;
            font-size: 16px;
            line-height: 19px;
            color: #666666;
            text-align: center;
        }

        .hover-icon,
        .hover-icon1 {
            display: none;
        }

        .individual-icon,
        .premium-icon {
            text-decoration: none;
        }





        @media (min-width:992px) and (max-width:1399px) {
            .register-img {
                margin-left: 23%;

            }
        }

        @media (max-width:576px) {
            .register_as {
                margin-top: 8%;
            }

            .register-img {
                margin-left: 23%;

            }

            .card {
                padding: 0;
            }

            .card10 {
                margin-left: 0%;
            }

            .card20 {
                margin-right: 0%;
            }

            .navbar {
                margin-top: 0 !important;
            }
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</head>

<body>
       <!-- navbar -->
    <%- include('include/login-navbar.ejs') %>
    <!-- navbar -->
 
    <div class="container">
        <div class="row">
            <h2 class="register_as">Register As</h2>
            <div class="col-lg-6">
                <a class="individual-icon" href="/individual">
                    <div class="card card10">
                        <div class="register-img real-icon">
                            <img src="assets/img/Group 19959.png" alt="">
                        </div>
                        <div class="register-img hover-icon">
                            <img src="assets/img/Group 19962.png" alt="">
                        </div>
                        <h2 class="register-heading">Individual User</h2>
                        <p class="register-pera">For individuals looking to connect to share interests, skills and their
                            stories.</p>
                    </div>
                </a>
            </div>
            <div class="col-lg-6">
                <a class="premium-icon" href="/commercial">
                    <div class="card card20">
                        <div class="register-img real-icon1">
                            <img src="assets/img/Group 19960.png" alt="">
                        </div>
                        <div class="register-img hover-icon1">
                            <img src="assets/img/Group 19961.png" alt="">
                        </div>
                        <h2 class="register-heading">Premium User</h2>
                        <p class="register-pera">For Premium looking to connect to share interests, skills and their
                            stories.
                        </p>
                    </div>
                </a>
            </div>
            <p class="login-now">Already Have An Account? <span><a href="/login">Login Now</a></span></p>
        </div>


    </div>




    <div class="container-fluid" style="padding: 0px 0px;">
        <div class="row" id="footer" style="margin: 30px 0px 0px 0px; padding:20px 20px;">
            <div class="col-12">
                Copyright Â© 2022 <a style="text-decoration: none; color: #225C7B;" href="/" id="login-link">Resshu Connect Pvt. Ltd.</a>  All Rights Reserved.
            </div>
        </div>
    </div>




    <script>


        $(document).ready(function () {


            $(".real-icon").mouseenter(enter);
            $(".real-icon").mouseleave(leave);
            function enter() {
                $(".real-icon").css("display", "none");
                $(".hover-icon").css("display", "block");
            }
            function leave() {
                $(".real-icon").css("display", "block");
                $(".hover-icon").css("display", "none");
            }


            $(".real-icon1").mouseenter(enter1);
            $(".real-icon1").mouseleave(leave1);
            function enter1() {
                $(".real-icon1").css("display", "none");
                $(".hover-icon1").css("display", "block");
            }
            function leave1() {
                $(".real-icon1").css("display", "block");
                $(".hover-icon1").css("display", "none");
            }



        });
    </script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</body>

</html>