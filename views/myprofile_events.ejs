        function profile_close() {
            $("#profile_opn").css("display", "none");

        }



        // search popup
        $(".search-popup").css("display", "none")

        function search_popup_open() {
            $(".search-popup").css("display", "block")

            $(".search-popup").mouseenter(enter);
            $(".search-popup").mouseleave(leave);
            function enter() {
                $(".search-popup").css("display", "block");
            }
            function leave() {
                $(".search-popup").css("display", "none");
            }
        }
        function search_popup_close() {
            // $(".search-popup").css("display", "none");
            $(".s-p-h-r").remove();
        }

        // Activity more popup
        // $(".activity-more-popup").css("display", "none")

        function activity_more_open() {
            $(".activity-more-popup").css("display", "block")

            $(".activity-more-popup").mouseenter(enter);
            $(".activity-more-popup").mouseleave(leave);
            function enter() {
                $(".activity-more-popup").css("display", "block");
            }
            function leave() {
                $(".activity-more-popup").css("display", "none");
            }
