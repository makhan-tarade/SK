  <style>
    body {
      background: #F1EFF0;
    }

    #navbar_nav {
      background: linear-gradient(90deg, #CCD7B5 0%, #ABB497 68.5%, #5B9092 100%);
    }

    #span_vector {
      font-family: 'Inter';
      font-style: normal;
      font-weight: 500;
      font-size: 18px;
      line-height: 22px;
      color: #444444;
      text-align: justify;
    }

    #adjust_span {
      margin: 10px, 20px, 10px, 20px;
    }

    #back-btn-class {
      font-family: 'Inter';
      font-style: normal;
      font-weight: 400;
      font-size: 18px;
      line-height: 22px;
      text-decoration: none;
      color: #225C7B;
    }

    #login-txt {
      font-family: 'Inter';
      font-style: normal;
      font-weight: 700;
      font-size: 24px;
      line-height: 29px;
      color: #222222;
      margin: 0px 0px 25px 0px;
    }

    input {
      width: 90% !important;
      margin: auto;
      border: 1px solid #C4C3C7;
      border-radius: 5px;
      padding: 15px 25px;
      font-family: 'Inter';
      font-style: normal;
      font-weight: 500;
      font-size: 16px;
      line-height: 19px;
      color: #444444;

    }

    input::placeholder {
      font-family: 'Inter';
      font-style: normal;
      font-weight: 400;
      font-size: 16px;
      line-height: 19px;

      color: #C4C3C7;
    }

    #inputtxt {
      font-family: 'Inter';
      font-style: normal;
      font-weight: 400;
      font-size: 16px;
      line-height: 19px;

      color: #C4C3C7;
    }


    input {
      box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.15);
      border-radius: 4px;
    }

    label {

      font-family: 'Inter';
      font-style: normal;
      font-weight: 400;
      font-size: 14px;
      line-height: 17px;
      color: #444444;

      margin-left: 4%;
      margin-bottom: 10px;

    }

    .form-group {
      margin-left: 4%;
    }

    input {
      width: 95% !important;
      color: #222222 !important;
    }

    input::placeholder {
      font-family: 'Inter';
      font-style: normal;
      font-weight: 400;
      font-size: 16px !important;
      line-height: 19px;

      color: #C4C3C7 !important;

    }

    .request-otp {

      font-family: 'Inter';
      font-style: normal;
      font-weight: 700;
      font-size: 16px;
      line-height: 19px;
      color: #FFFFFF;
      background-color: #225C7B;

      margin-top: 5%;
    }

    .request-otp:hover {
      background-color: #225C7B;
    }

    .request-otp:focus {
      box-shadow: none !important;
    }


    @media(max-width:576px) {
      input {
        width: 100% !important;
      }

      .navbar {
        margin-top: -25px !important;
      }

      .request-otp {

        margin-top: 15%;
      }

      /* .mt-4{
