                <input type="text" id="mobile_code" class="form-control" placeholder="Mobile Number" name="mobile"
                  value="<% if (mobile) { %><%= mobile %><% } %>" pattern="[6789][0-9]{9}" maxlength="10" required>

                <div class="invalid-feedback">
                  Please provide a valid city.
                </div>

              </div>
              <center>
                <button id="req_otp" class="btn btn-primary request-otp">Request OTP</button>
              </center>

            </form>


            <div class="row" style="margin-top: 300px;text-align: center;">
              <p style="font-family: 'Inter';
                font-style: normal;
                font-weight: 400;
