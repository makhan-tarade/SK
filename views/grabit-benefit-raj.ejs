    <style>
               .sec-heading{
            font-family: 'Inter';
            font-style: normal;
            font-weight: 600;
            font-size: 16px;
            padding: 12px 49px;
            margin: 0 10%;
            margin-top: 15px;
            margin-bottom: 30px;
            margin-left: -7px;
            border: 2px solid transparent;
            border-radius: 42px;
            color: #9C9C9C;
            /* text-transform: capitalize; */
        }


        .sec-heading:hover{
         
            border: 2px solid #225C7B;
       
            color: #323232;
            text-transform: capitalize;
        }

        .sec-heading:active{
            
            border: 2px solid #225C7B; 
        
            color: #615f5f;
   
        }


